{
  "name": "yggjs-gm",
  "version": "0.1.0",
  "description": "国密算法 TypeScript 库 - 支持 SM2/SM3/SM4 算法的现代化实现",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/browser.esm.js",
  "unpkg": "./dist/browser.umd.js",
  "jsdelivr": "./dist/browser.umd.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./node": {
      "types": "./dist/adapters/node.d.ts",
      "import": "./dist/adapters/node.js",
      "require": "./dist/adapters/node.cjs"
    },
    "./browser": {
      "types": "./dist/adapters/browser.d.ts",
      "import": "./dist/adapters/browser.js"
    },
    "./core/sm2": {
      "types": "./dist/core/sm2/index.d.ts",
      "import": "./dist/core/sm2/index.js",
      "require": "./dist/core/sm2/index.cjs"
    },
    "./core/sm3": {
      "types": "./dist/core/sm3/index.d.ts",
      "import": "./dist/core/sm3/index.js",
      "require": "./dist/core/sm3/index.cjs"
    },
    "./core/sm4": {
      "types": "./dist/core/sm4/index.d.ts",
      "import": "./dist/core/sm4/index.js",
      "require": "./dist/core/sm4/index.cjs"
    },
    "./core/math": {
      "types": "./dist/core/math/index.d.ts",
      "import": "./dist/core/math/index.js",
      "require": "./dist/core/math/index.cjs"
    },
    "./api/sm2": {
      "types": "./dist/api/sm2.d.ts",
      "import": "./dist/api/sm2.js",
      "require": "./dist/api/sm2.cjs"
    },
    "./api/sm3": {
      "types": "./dist/api/sm3.d.ts",
      "import": "./dist/api/sm3.js",
      "require": "./dist/api/sm3.cjs"
    },
    "./api/sm4": {
      "types": "./dist/api/sm4.d.ts",
      "import": "./dist/api/sm4.js",
      "require": "./dist/api/sm4.cjs"
    },
    "./api/key": {
      "types": "./dist/api/key.d.ts",
      "import": "./dist/api/key.js",
      "require": "./dist/api/key.cjs"
    },
    "./api/random": {
      "types": "./dist/api/random.d.ts",
      "import": "./dist/api/random.js",
      "require": "./dist/api/random.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:prod": "cross-env NODE_ENV=production tsup --minify",
    "build:rollup": "rollup -c",
    "build:rollup:prod": "cross-env NODE_ENV=production rollup -c",
    "build:rollup:analyze": "cross-env ANALYZE=true NODE_ENV=production rollup -c",
    "build:all": "pnpm build:prod && pnpm build:rollup:prod",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:browser": "vitest --config vitest.browser.config.ts",
    "test:browser:coverage": "vitest --config vitest.browser.config.ts --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:cross-lang": "vitest tests/cross-lang",
    "test:all": "pnpm test && pnpm test:browser && pnpm test:e2e",
    "lint": "echo 'ESLint disabled for easier development'",
    "lint:fix": "echo 'ESLint disabled for easier development'",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "docs:dev": "typedoc --watch",
    "docs:build": "typedoc",
    "benchmark": "node scripts/benchmark.js",
    "size-check": "pnpm build:rollup:analyze",
    "prepublishOnly": "pnpm run build:all"
  },
  "keywords": [
    "cryptography",
    "sm2",
    "sm3",
    "sm4",
    "gm",
    "china",
    "typescript",
    "crypto",
    "elliptic-curve",
    "hash",
    "cipher"
  ],
  "author": "YggJS Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yggdrasil-gm/yggjs-gm.git"
  },
  "bugs": {
    "url": "https://github.com/yggdrasil-gm/yggjs-gm/issues"
  },
  "homepage": "https://github.com/yggdrasil-gm/yggjs-gm#readme",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.0.0",
    "happy-dom": "^12.0.0",
    "jsdom": "^23.0.0",
    "playwright": "^1.40.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-filesize": "^10.0.0",
    "tslib": "^2.8.1",
    "tsup": "^8.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "yggjs-gm": "file:yggjs-gm-0.1.0.tgz",
    "yggjs_gm": "file:yggjs-gm-0.1.0.tgz"
  },
  "sideEffects": false,
  "dependencies": {
    "big-integer": "^1.6.52"
  }
}
